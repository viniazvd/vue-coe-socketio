{
  "name": "gitty",
  "version": "3.6.0",
  "description": "A Node.js wrapper for the Git CLI",
  "main": "index",
  "dependencies": {},
  "devDependencies": {
    "gh-pages": "^0.10.0",
    "ink-docstrap": "^1.1.4",
    "jsdoc": "^3.4.0",
    "mocha": "^1.21.4",
    "pushover": "^1.3.6",
    "rimraf": "^2.2.8",
    "should": "^4.0.4"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha test/* -R spec",
    "make-docs": "./node_modules/.bin/jsdoc index.js lib -r -R README.md -c .jsdoc.json --verbose -d ./jsdoc",
    "publish-docs": "npm run make-docs && node script/publishdoc.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bookchin/gitty.git"
  },
